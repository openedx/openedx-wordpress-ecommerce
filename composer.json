{
	"name": "php-actions/example-phpunit",
	"description": "Example project using php-actions/phpunit",

	"require-dev": {
		"ext-curl": "*",
		"ext-dom": "*",
		"ext-json": "*",
		"ext-sqlite3": "*",
		"phpunit/phpunit": "9.*"
	},
	"autoload": {
		"psr-4": {
			"App\\": "includes/",
			"App\\model\\": "includes/model/",
			"App\\admin\\": "admin/",
			"App\\admin\\views\\": "admin/views/",
			"App\\public\\": "public/",
			"App\\utils\\": "utils/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "test/"
		}
	},
	"require": {
		"guzzlehttp/guzzle": "^7.7"
	}
}
